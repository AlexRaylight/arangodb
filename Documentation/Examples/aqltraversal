TRAVERSE(friends, friendrelations, "friends/john", "outbound", {
  strategy: "depthfirst",
  order: "postorder",
  itemOrder: "backward",
  maxDepth: 6,
  trackPaths: true
})

TRAVERSE(friends, friendrelations, "friends/john", "outbound", {
  order: "breadthfirst",
  order: "preorder",
  itemOrder: "forward",
  followEdges: [ { type: "knows" }, { state: "FL" } ]
})


