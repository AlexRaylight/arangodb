avocado> var Graph = require("graph").Graph;

avocado> g = new Graph("vertices", "edges");
Graph("vertices", "edges")

avocado> v = g.addVertex();
Vertex(<graph>, "153246:2310672")

avocado> e = g.addEdge(v, v, "self", { weight: 10 })
